   "source": [
    "def count_string(string, substring):\n",
    "\n",
    "\n",
    "\n",
    "    count=0\n",
    "\n",
    "    for i in range(len(string)):\n",
    "\n",
    "       \n",
    "\n",
    "        if(string[i:].startswith(substring)):\n",
    "\n",
    "            count=count+1\n",
    "\n",
    "    return count\n",
    "\n",
    "\n",
    "\n",
    "s1=input(\"Enter the String : \")\n",
    "\n",
    "s2=input(\"Enter the Substring : \")\n",
    "\n",
    "print(count_string(s1,s2))"
   ]
